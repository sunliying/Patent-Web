<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <servlet>
    <servlet-name>ParentsSearcher</servlet-name>
    <servlet-class>search.ParentsSearcherServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SpicificServlet</servlet-name>
    <servlet-class>search.SpicificServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>visualizationServlet</servlet-name>
    <servlet-class>dataVisualization.visualizationServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SearchAnalysis</servlet-name>
    <servlet-class>dynamicChartAnal.SearchAnalysis</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>ParentsSearcher</servlet-name>
    <url-pattern>/ParentsSearcher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SpicificServlet</servlet-name>
    <url-pattern>/SpicificServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>visualizationServlet</servlet-name>
    <url-pattern>/visualizationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchAnalysis</servlet-name>
    <url-pattern>/SearchAnalysis</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
